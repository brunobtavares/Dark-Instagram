{
    "manifest_version": 2,
    "name": "Instagram Dark Theme",
    "description": "Deixei seu instagram com modo noturno",
    "version": "0.0",
    "browser_action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "css": [
                "index.css"
            ],
            "js": [
                "index.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ],
    "permissions": [
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "bi16.png",
        "48": "bi48.png",
        "128": "bi128.png"
    }
}